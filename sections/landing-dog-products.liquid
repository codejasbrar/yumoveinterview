<style>
.sticky {
	position: fixed;
  	top: calc(50vh - 150px);
  	left: 0;
    height: 300px;
  	opacity: 0;
  	transition: opacity 300ms ease-in;
}
.sticky span {
  transform: rotate( -90deg);
  transform-origin: top left;
  width: 300px;
  font-size: 25px;
  border-top-left-radius: 0;
  border-top-right-radius: 0;
  display: block;
  margin-top: 300px;
  transition: background 300ms ease;
}
  .sl-product--dog {
    display: flex!important;
  }
  .slider-dogproducts-nav, .slider-dogproducts-nav .slick-list {
  	width: 100%;
    max-width: 800px;
    margin-left: auto;
    margin-right: auto;
  }
  .slider-dogproducts-nav {
  	margin-top: -35px;
    margin-bottom: 35px;
  }
  .slider-dogproducts-nav__item {
  	display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    text-align: center;
    cursor: pointer;
  }

  .slider-dogproducts-nav__item img {
  	margin: 0 auto;
    height: 154px;
    width: auto;
  }
  .slider-dogproducts-nav__item h3 {
  	font-size: 18px;
  }
  .slider-dogproducts-nav .slick-slide {
  	opacity: 0.5;
  }
  .slider-dogproducts-nav .slick-slide.slick-current {
  	opacity: 1;
  }
  .slider-dogproducts-nav .slick-slide:hover {
  	opacity: 1;
  }
  .slick-arrow {
  	position: absolute;
    top: 10%;
    z-index: 9;
    cursor: pointer;
  }
  .slick-arrow:hover {
  	opacity: 0.8;
  }
  .fa-arrow-left {
  	left: 0;
  }
  .fa-arrow-right {
  	right: 0;
  }
  
  @media(min-width: 768px) {
    .slick-arrow {
      top: 50%;
      transform: translateY(-50%);
    }
  }
</style>
