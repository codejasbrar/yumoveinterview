{%- assign _section = section.settings -%}
{%- assign section_title = _section.title -%}

<h1 style="position: absolute; opacity:0; pointer-events: none;">{{section_title}}</h1>

{% schema %}
{
  "name": "Hidden H1",
  "settings": [
    {
      "type": "text",
      "id": "title",
      "label": "Title"
    }
  ]
}
{% endschema %}